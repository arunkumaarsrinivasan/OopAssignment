import java.io.ByteArrayInputStream;
public class A
{
public static void main(String args[])
{
try
{
byte[] array={10,20,30};
ByteArrayInputStream in=new ByteArrayInputStream(array);
for(int i=0;i<array.length;i++)
{
int data=in.read();
System.out.println(data);
}
}
catch(Exception e)
{
System.out.println(e);
} }}
