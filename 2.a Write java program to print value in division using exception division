class A 
{ 
public static void main(String args[]) 
{ 
try 
{ 
int a=10, b=0,c; 
c=a/b; 
System.out.println(c); 
try 
{ 
int x[] = {1, 2, 3, 4, 5 }; 
System.out.println(x[10]); 
} 
catch(Exception e1) 
{ 
System.out.println("Index out of range"); 
} 
} 
catch(Exception e2) 
{ 
System.out.println("Divided by zero"); 
} 
}
}

